{
  "name": "rabbitmq-delete-message",
  "version": "0.0.2",
  "description": "A tool to delete a message from a queue in RabbitMQ",
  "main": "build/index.js",
  "author": {
    "email": "rgoyard@gmail.com",
    "name": "RÃ©mi Goyard",
    "url": "https://www.mimiz.fr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mimiz/rabbitmq-delete-message.git"
  },
  "keywords": [
    "RabbitMQ",
    "event",
    "message"
  ],
  "types": "build/index.d.js",
  "license": "MIT",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "rimraf ./build && tsc",
    "serve": "nodemon",
    "server": "nodemon --config nodemon.server.json",
    "lint": "eslint . --ext .ts",
    "start": "npm run build && node build/index.js",
    "test": "cross-env RABBITMQ_DELETE_MESSAGE_TIMEOUT=1 NODE_ENV=test mocha --exit ./test/**/*.test.ts",
    "coverage": "nyc yarn test",
    "prepare": "yarn build",
    "preversion": "yarn lint",
    "version": "yarn prettier-format && git add -A src",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "yarn test && yarn lint",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "waait": "^1.0.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.2",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}